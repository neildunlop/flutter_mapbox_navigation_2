<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fahad.app">
    
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
   <application
        android:label="fluttermapboxnavigationexample"
        android:icon="@mipmap/ic_launcher">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:launchMode="singleTop"
            android:theme="@style/LaunchTheme"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|smallestScreenSize|locale|layoutDirection|fontScale|screenLayout|density|uiMode"
            android:hardwareAccelerated="true"
            android:windowSoftInputMode="adjustResize">
            <!-- Specifies an Android theme to apply to this Activity as soon as
                 the Android process has started. This theme is visible to the user
                 while the Flutter UI initializes. After that, this theme continues
                 to determine the Window background behind the Flutter UI. -->
            <meta-data
              android:name="io.flutter.embedding.android.NormalTheme"
              android:resource="@style/NormalTheme"
              />
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <!-- Navigation Activity -->
        <activity 
            android:name="com.eopeter.fluttermapboxnavigation.activity.NavigationActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            tools:replace="android:theme" />
        
        <receiver
            android:name="com.eopeter.fluttermapboxnavigation.receivers.NavigationReceiver"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />
            
        <receiver
            android:name="com.mapbox.navigation.core.trip.service.NavigationNotificationReceiver"
            android:exported="false" />
            
        <receiver
            android:name="com.mapbox.navigation.core.trip.service.NavigationServiceReceiver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.service.NavigationServiceBroadcastReceiver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.service.NavigationServiceNotificationReceiver"
            android:exported="false" />

        <!-- Dynamic receivers from NavigationActivity -->
        <receiver
            android:name="com.eopeter.fluttermapboxnavigation.activity.NavigationActivity$1"
            android:exported="false">
            <intent-filter>
                <action android:name="com.eopeter.fluttermapboxnavigation.activity.NavigationLauncher.KEY_STOP_NAVIGATION" />
            </intent-filter>
        </receiver>

        <receiver
            android:name="com.eopeter.fluttermapboxnavigation.activity.NavigationActivity$2"
            android:exported="false">
            <intent-filter>
                <action android:name="com.eopeter.fluttermapboxnavigation.activity.NavigationLauncher.KEY_ADD_WAYPOINTS" />
            </intent-filter>
        </receiver>
            
        <!-- Navigation observers -->
        <receiver
            android:name="com.mapbox.navigation.core.trip.session.BannerInstructionsObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.session.VoiceInstructionsObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.session.OffRouteObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.session.RouteProgressObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.session.LocationObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.arrival.ArrivalObserver"
            android:exported="false" />

        <!-- Additional observers -->
        <receiver
            android:name="com.mapbox.navigation.core.directions.session.RoutesObserver"
            android:exported="false" />

        <receiver
            android:name="com.mapbox.navigation.core.trip.session.MapViewObserver"
            android:exported="false" />
            
        <!-- Don't delete the meta-data below.
             This is used by the Flutter tool to generate GeneratedPluginRegistrant.java -->
        <meta-data
            android:name="flutterEmbedding"
            android:value="2" />
    </application>
</manifest>
